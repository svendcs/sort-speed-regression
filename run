# Compile TPIE Master
cd tpie-master/build
cmake -DCMAKE_BUILD_TYPE=Release ..
make -j9 tpie
cd ../..

# Compile TPIE Faster
cd tpie-faster/build
cmake -DCMAKE_BUILD_TYPE=Release ..
make -j9 tpie
cd ../..

# Make test
make -j9

# Run
echo "--- MASTER SORTING ---"
./bin/master 5 $1 $2

echo "--- EVEN-FASTER-SORTING ---"
./bin/faster 5 $1 $2
