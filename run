# Compile TPIE Master
cd tpie-master/build
cmake -DCMAKE_BUILD_TYPE=Release ..
make -j9 tpie
cd ../..

# Compile TPIE Faster
cd tpie-faster/build
cmake -DCMAKE_BUILD_TYPE=Release ..
make -j9 tpie
cd ../..

# Make test
make -j9

# Run
if [ "$1" != "faster" ]; then
	echo "--- MASTER SORTING ---"
	./bin/master 3 20000 4000;
fi

if [ "$1" != "master" ]; then
	echo "--- EVEN-FASTER-SORTING ---";
	./bin/faster 3 20000 4000;
fi
